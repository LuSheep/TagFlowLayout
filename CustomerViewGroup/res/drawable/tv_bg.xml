<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
	 <item android:state_pressed="false">
        <shape>
		    <solid android:color="#7690A5" />  
		    <corners android:radius="5dp"/>  
		    <padding  
		        android:bottom="2dp"  
		        android:left="10dp"  
		        android:right="10dp"  
		        android:top="2dp" />  
		</shape>
    </item>
    
    <item android:state_pressed="true">
        <shape>
		    <solid android:color="#768080" />  
		    <corners android:radius="5dp"/>  
		    <padding  
		        android:bottom="2dp"  
		        android:left="10dp"  
		        android:right="10dp"  
		        android:top="2dp" />  
		</shape>
    </item>   
</selector>
